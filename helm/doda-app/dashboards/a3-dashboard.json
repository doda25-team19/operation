{
  "id": null,
  "uid": "doda-a3",
  "title": "DODA A3 monitoring",
  "tags": ["doda", "a3", "monitoring"],
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Requests per second by version",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (version) (rate(doda_predictions_total[$__rate_interval]))",
          "legendFormat": "{{version}}"
        }
      ]
    },
    {
      "id": 2,
      "title": "Error ratio by version",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (version) (rate(doda_predictions_total{result=\"error\"}[$__rate_interval])) / sum by (version) (rate(doda_predictions_total[$__rate_interval]))",
          "legendFormat": "{{version}}"
        }
      ]
    },
    {
      "id": 3,
      "title": "Latency p90 by version",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "refId": "P90",
          "expr": "histogram_quantile(0.9, sum(rate(doda_prediction_duration_seconds_bucket[$__rate_interval])) by (le, version))",
          "legendFormat": "p90 {{version}}"
        }
      ]
    }
  ],
  "templating": {
    "list": []
  }
}
