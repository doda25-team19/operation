{
  "id": null,
  "uid": "doda-overview",
  "title": "DODA overview",
  "tags": ["doda", "overview"],
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 2,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Requests by result",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (result) (rate(doda_predictions_total[$__rate_interval]))",
          "legendFormat": "{{result}}"
        }
      ]
    },
    {
      "id": 2,
      "title": "Error ratio",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(doda_predictions_total{result=\"error\"}[$__rate_interval])) / sum(rate(doda_predictions_total[$__rate_interval]))",
          "legendFormat": "error ratio"
        }
      ]
    },
    {
      "id": 3,
      "title": "Latency quantiles",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 16 },
      "targets": [
        {
          "refId": "P50",
          "expr": "histogram_quantile(0.5, sum(rate(doda_prediction_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p50"
        },
        {
          "refId": "P90",
          "expr": "histogram_quantile(0.9, sum(rate(doda_prediction_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p90"
        },
        {
          "refId": "P99",
          "expr": "histogram_quantile(0.99, sum(rate(doda_prediction_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p99"
        }
      ]
    }
  ],
  "templating": {
    "list": []
  }
}
