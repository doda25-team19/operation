{
  "id": null,
  "uid": "doda-overview",
  "title": "DODA overview",
  "tags": ["doda"],
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Predictions per result",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (result) (rate(doda_predictions_total[5m]))",
          "legendFormat": "{{result}}"
        }
      ]
    },
    {
      "id": 2,
      "title": "Current input text length",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "doda_input_text_length",
          "legendFormat": "length"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "title": "Prediction latency quantiles",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "refId": "P50",
          "expr": "histogram_quantile(0.5, sum(rate(doda_prediction_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "refId": "P90",
          "expr": "histogram_quantile(0.9, sum(rate(doda_prediction_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p90"
        },
        {
          "refId": "P99",
          "expr": "histogram_quantile(0.99, sum(rate(doda_prediction_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "id": 4,
      "title": "Total predictions by result",
      "type": "barchart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 17 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (result) (doda_predictions_total)",
          "legendFormat": "{{result}}"
        }
      ],
      "options": {
        "orientation": "horizontal"
      }
    }
  ],
  "templating": {
    "list": []
  }
}
